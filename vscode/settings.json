{
    "[buildconfig]": {
        "editor.formatOnType": true,
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "off",
            "strings": "off"
        }
    },
    "[c]": {
        "editor.semanticHighlighting.enabled": false
    },
    "[systemverilog]": {
        "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
        "editor.formatOnSave": false,
        "editor.formatOnType": true,
        "editor.quickSuggestions": {
            "other": "on"
        }
    },
    "[vhdl]": {
        "editor.formatOnType": true,
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "off",
            "strings": "off"
        }
    },
    "accessibility.signals.lineHasInlineSuggestion": {
        "sound": "off"
    },
    "better-comments.tags": [
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#FF2D00",
            "italic": false,
            "strikethrough": false,
            "tag": "!",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#3498DB",
            "italic": false,
            "strikethrough": false,
            "tag": "?",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": true,
            "color": "#FFD700",
            "italic": false,
            "strikethrough": false,
            "tag": "todo",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#98C379",
            "italic": false,
            "strikethrough": false,
            "tag": "*",
            "underline": false
        }
    ],
    "bookmarks.experimental.enableNewStickyEngine": false,
    "bookmarks.label.suggestion": "useWhenSelected",
    "bookmarks.saveBookmarksInProject": true,
    "bookmarks.sideBar.expanded": true,
    "bookmarks.wrapNavigation": false,
    "chat.commandCenter.enabled": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "DVT.contentFilters.enable": true,
    "DVT.incrementalProfiling.displayReportIssueNotification": false,
    "DVT.internal.preferences": {
        "dvt.vscode.update.1.93.1": true
    },
    "DVT.license.source": "/opt/lic/share/license/site/lmadmin/amiq/amiq_lic",
    "DVT.output.automaticallyShowBuild": false,
    "DVT.problems.maximumNumberOfDiagnostics": 10,
    "DVT.textEditor.highlight.enableInactiveGenerateBranchesHighlight": false,
    "DVT.textEditor.systemVerilog.contentAssist.auto-Instantiation.sortPortsAlphabetically": false,
    "DVT.textEditor.systemVerilog.contentAssist.ignoreCase": false,
    "DVT.textEditor.systemVerilog.contentAssist.insertMatchingVariablesAsArguments": false,
    "DVT.textEditor.systemVerilog.contentAssist.insertNamedArgumentsAsMultiline": false,
    "DVT.textEditor.systemVerilog.contentAssist.insertOptionalArguments": false,
    "DVT.textEditor.systemVerilog.formatting.indentation.doNotIndentSingleLineCommentsAtBeginningOfLine": false,
    "DVT.textEditor.systemVerilog.formatting.indentation.noIndentKeywords": {
        "module": true
    },
    "DVT.textEditor.systemVerilog.formatting.indentation.preprocessing": "Indent and increase indentation level - except first",
    "DVT.textEditor.systemVerilog.formatting.verticalAlignment.enableVerticalAlignment": true,
    "DVT.textEditor.systemVerilog.formatting.verticalAlignment.independentPreprocessingScopes": true,
    "DVT.textEditor.systemVerilog.formatting.verticalAlignment.onlyConsecutiveLines": true,
    "DVT.textEditor.systemVerilog.formatting.verticalAlignment.verticalAlignPatterns": {
        "By Name Port Connections": true,
        "Class Variable Declarations (split dimensions)": true
    },
    "DVT.textEditor.systemVerilog.formatting.verticalAlignment.verticalAlignTokens": [
        ":=",
        "|",
        "&",
        "||",
        "&&",
        "|=",
        "&="
    ],
    "DVT.textEditor.systemVerilog.formatting.verticalAlignment.verticalAlignToOpenParenthesis": false,
    "DVT.textEditor.systemVerilog.formatting.whitespace.addWhitespaceAfter": [
        "== <= || && + , < > <= ;"
    ],
    "DVT.textEditor.systemVerilog.formatting.whitespace.addWhitespaceBefore": [
        "== <= || && + < > <= | &"
    ],
    "DVT.textEditor.systemVerilog.formatting.whitespace.compactWhitespaceGroups": true,
    "DVT.textEditor.systemVerilog.highlight.enableInactiveCodeHighlight": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.colorDecorators": false,
    "editor.cursorSurroundingLines": 16,
    "editor.find.seedSearchStringFromSelection": "selection",
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.minimap.showSlider": "always",
    "editor.occurrencesHighlight": "off",
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "none",
    "editor.scrollbar.horizontal": "visible",
    "editor.scrollbar.vertical": "visible",
    "editor.semanticTokenColorCustomizations": {
        "[Bear Theme]": {
            "enabled": true,
            "rules": {
                "class": "#ff5dab",
                "CONSTANT": {
                    "fontStyle": "bold",
                    "foreground": "#82AAFF"
                },
                "FIELD": "#d7d5d5",
                "OUT_PORT": "#ff5dab",
                "TYPE": "#4EC9B0",
                "variable": "#d7d5d5",
                "VARIABLE": "#d7d5d5"
            }
        }
    },
    "editor.stickyScroll.enabled": true,
    "editor.tabSize": 4,
    "editor.tokenColorCustomizations": {
        "[Bear Theme]": {
            "keywords": "#ff3a3a",
            "strings": "#60e860",
            "textMateRules": [
                {
                    "scope": [
                        "keyword.operator",
                        "keyword.operator.assignment",
                        "punctuation.separator.delimiter.c",
                        "variable.other.readwrite.verilog"
