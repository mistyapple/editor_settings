    "logFileHighlighter.customPatterns": [
        {
            "foreground": "#d4d89df6",
            "pattern": "/\\b[a-zA-z0-9/._@-]*[/.]\\w*"
        },
        {
            "foreground": "#4EC9B0",
            "pattern": "\\b(PASSING|Passing|PASSED|Passed|PASS|Pass)\\b"
        },
        {
            "foreground": "#CE9178",
            "pattern": "\\b(UVM_WARNING|OVM_WARNING|Warning|MVC_WARNING)\\b"
        },
        {
            "foreground": "#CE9178",
            "pattern": "\\*W,\\S+"
        },
        {
            "fontWeight": "bold",
            "foreground": "#FF3A3A",
            "pattern": "\\b(ERROR|UVM_ERROR|UVM_FATAL|MVC_ERROR|OVM_ERROR|OVM_FATAL|FAIL|FAILURE|Failure|Failed|FAILED|Failing|FAILING)\\b"
        },
        {
            "fontWeight": "bold",
            "foreground": "#FF3A3A",
            "pattern": "\\*(S)?E,\\S+"
        },
        {
            "fontWeight": "bold",
            "foreground": "#FF3A3A",
            "pattern": "\\*(S)?F,\\S+"
        },
        {
            "foreground": "#B5CEA8",
            "pattern": "(UVM_INFO|OVM_INFO)"
        },
        {
            "foreground": "#f0a94d",
            "pattern": "DEBUG*"
        }
    ],
    "multiCommand.commands": [
        {
            "command": "multiCommand.reverseComments",
            "sequence": [
