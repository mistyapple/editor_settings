                    "settings": {
                        "foreground": "#FFCB6B"
                    }
                },
                {
                    "scope": [
                        "constant.language",
                        "constant.other.caps.python",
                        "constant.other.define.systemverilog",
                        "constant.other.net.systemverilog",
                        "constant.other.systemverilog",
                        "entity.name.function.preprocessor.c",
                        "entity.name.type.define.systemverilog",
                        "log.date",
                        "variable.other.constant.verilog"
