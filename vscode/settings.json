                        "keyword.operator.other.systemverilog",
                        "keyword.operator.word.systemverilog",
                        "keyword.other.systemverilog",
                        "keyword.other.verilog",
                        "log.error"
                    ],
                    "settings": {
                        "foreground": "#ff3a3a"
                    }
                },
                {
                    "scope": [
                        "entity.name.type.c",
                        "entity.name.type.cpp",
                        "entity.name.type.parametrizable.systemverilog",
                        "storage.modifier",
                        "storage.type.built-in",
                        "storage.type.interface.systemverilog",
                        "storage.type.net-type.systemverilog",
                        "storage.type.systemverilog",
                        "storage.type.userdefined.systemverilog",
                        "storage.type.variable.systemverilog",
                        "support.type.built-in.posix-reserved",
                        "support.type.posix-reserved.c",
                        "support.type.posix-reserved.systemverilog",
                        "support.type.python",
                        "support.type.systemverilog",
                        "support.type.uvm.uvm-classes.systemverilog",
                        "support.type.verilog"
                    ],
                    "settings": {
                        "foreground": "#4EC9B0"
                    }
                },
                {
                    "scope": [
                        "constant.other.preprocessor.systemverilog",
                        "entity.name.section.systemverilog",
                        "keyword.operator.delay.verilog",
                        "keyword.operator.timing.systemverilog",
                        "keyword.other.systemverilog",
                        "meta.indexed-name.python",
                        "support.function.user-defined-macro.systemverilog",
                        "support.function.uvm-user-defined-macro.systemverilog",
                        "support.function.uvm.macro.systemverilog",
                        "variable.object.c",
                        "variable.other.object"
                    ],
                    "settings": {
                        "foreground": "#f7afca"
                    }
                },
                {
                    "scope": [
                        "constant.character.escape.systemverilog",
                        "constant.numeric",
                        "constant.numeric.bin.verilog",
                        "constant.numeric.dec.verilog",
                        "constant.numeric.decimal.systemverilog",
                        "constant.numeric.hex.verilog",
                        "constant.numeric.systemverilog",
                        "constant.other.placeholder.systemverilog",
                        "constant.other.placeholder.verilog",
                        "keyword.other.unit.hexadecimal",
                        "keyword.other.unit.suffix.integer.c",
                        "storage.type.number.verilog"
                    ],
                    "settings": {
                        "foreground": "#F3F99D"
                    }
                },
                {
                    "scope": [
                        "storage.type.number.verilog"
                    ],
                    "settings": {
                        "foreground": "#F3F99D"
                    }
                },
                {
                    "scope": [
                        "storage.modifier.systemverilog"
                    ],
                    "settings": {
                        "foreground": "#ff5dab"
                    }
                },
                {
                    "scope": [
                        "entity.name.namespace.systemverilog",
                        "entity.name.namespaces.systemverilog",
                        "keyword.other.port.verilog",
                        "log.constant",
                        "support.class.systemverilog",
                        "support.type"
                    ],
                    "settings": {
                        "foreground": "#ff5dab"
                    }
                },
                {
                    "scope": [
                        "entity.name.scope-resolution.cpp",
                        "keyword.operator.arithmetic.python",
                        "keyword.operator.arithmetic.systemverilog",
                        "keyword.operator.bitwise.shift.c",
                        "keyword.operator.bitwise.systemverilog",
                        "keyword.operator.bitwise.verilog",
                        "keyword.operator.c",
                        "keyword.operator.logical.c",
                        "keyword.operator.logical.systemverilog",
                        "keyword.operator.logical.verilog",
                        "keyword.operator.logical.python",
                        "support.function.magic.python"
